{
  "name": "poe-overlay",
  "description": "A Overlay for Path of Exile. Built with Electron and Angular.",
  "homepage": "https://github.com/Kyusung4698/PoE-Overlay",
  "repository": {
    "url": "https://github.com/Kyusung4698/PoE-Overlay"
  },
  "author": {
    "name": "Kyusung4698"
  },
  "version": "0.5.14",
  "scripts": {
    "postinstall": "npm run ng:cc && npm run electron:deps",
    "ng:cc": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "ng:serve": "ng serve",
    "ng:test": "ng test",
    "ng:lint": "ng lint --fix",
    "start": "npm-run-all -p ng:serve electron:serve",
    "build": "npm run electron:serve-tsc && ng build",
    "build:dev": "npm run build",
    "build:prod": "npm run build -- -c production",
    "electron:serve-tsc": "tsc -p tsconfig.serve.json",
    "electron:serve": "wait-on http-get://localhost:4200/ && npm run electron:serve-tsc && electron . --serve",
    "electron:local": "npm run build:prod && electron .",
    "electron:windows": "npm run build:prod && electron-builder build --windows",
    "electron:version": "electron -v && electron -a",
    "electron:deps": "electron-builder install-app-deps",
    "electron:rebuild": "npm rebuild hotkeys robotjs --runtime=electron --target=8.0.0 --disturl=https://atom.io/download/atom-shell --abi=76"
  },
  "main": "main.js",
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.0.0",
    "@angular/cdk": "~9.0.0",
    "@angular/common": "~9.0.0",
    "@angular/compiler": "~9.0.0",
    "@angular/core": "~9.0.0",
    "@angular/forms": "~9.0.0",
    "@angular/material": "^9.0.0",
    "@angular/platform-browser": "~9.0.0",
    "@angular/platform-browser-dynamic": "~9.0.0",
    "@angular/router": "~9.0.0",
    "@ngx-translate/core": "^12.0.0",
    "@swimlane/ngx-charts": "^13.0.2",
    "hotkeys": "git+https://github.com/Kyusung4698/hotkeys.git#4fe60fd9cf629d32bb2363de0b1047056953a4c3",
    "localforage": "^1.7.3",
    "moment": "^2.24.0",
    "robotjs": "git+https://github.com/Kyusung4698/robotjs.git#81ca63005b48acb7b4fdb495d90cd17ac1a48e49",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.1",
    "@angular/cli": "^9.0.1",
    "@angular/compiler-cli": "~9.0.0",
    "@angular/language-service": "~9.0.0",
    "@types/jasmine": "~3.5.3",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.12.26",
    "codelyzer": "^5.1.2",
    "electron": "^8.0.0",
    "electron-builder": "^22.3.2",
    "electron-reload": "^1.5.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.1",
    "karma-jasmine": "~3.1.0",
    "karma-jasmine-html-reporter": "^1.5.2",
    "npm-run-all": "^4.1.5",
    "protractor": "^5.4.3",
    "ts-node": "^8.6.2",
    "tslint": "~6.0.0",
    "typescript": "~3.7.5",
    "wait-on": "^4.0.0"
  },
  "engines": {
    "node": ">=10.9.0"
  },
  "browser": {
    "fs": false
  }
}
